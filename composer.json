{
    "name": "lcbcruz/flarum-portuguese",
    "type": "flarum-extension",
    "description": "Portugal portuguese language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
	    {
		    "name": "Luís Cruz"
	    }
           
    ],
    "support": {
	    "issues": "https://github.com/lcbcruz/flarum-portuguese/issues",
	    "source": "https://github.com/lcbcruz/flarum-portuguese"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Português Portugal",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "pt-pt",
            "title": "Português Portugal"
        }
    }
}